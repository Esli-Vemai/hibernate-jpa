package com.in28minutes.learnspringboot.beans;

/**
 * @author esliv
 *
 */
public class Courses {
	private int id;
	private String name;
	private String author;
	public int getId() {
		return id;
	}
	public String getName() {
		return name;
	}
	public String getAuthor() {
		return author;
	}
	
	
	

}
